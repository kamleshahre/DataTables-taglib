<!doctype html>
<html>
<head>
  <meta content="text/html;charset=UTF-8" http-equiv="Content-Type" />
  <meta name="description" content="JSP taglib for DataTables, an awesome JQuery plugin" />
  <meta name="author" content="Thibault Duchateau" />
  <meta name="keywords" content="web,java,jee,taglib,jquery,datatables,tag,bootstrap" />
  
  <title><%= @document.title or "DataTables Taglib" %></title>
  
  <link href="/DataTables-taglib/css/fwk/bootstrap.min.css" rel="stylesheet">
  <% for css in @document.styles : %>
  <link href="/DataTables-taglib<%= css %>" rel="stylesheet">
  <% end %>

  <style type="text/css">
    body {
      padding-top: 60px;
      padding-bottom: 40px;
    }
  </style>
  
  <!-- Le HTML5 shim, for IE6-8 support of HTML5 elements -->
  <!--[if lt IE 9]>
  <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
  <![endif]-->
  
  <script type="text/javascript">
    var _gaq = _gaq || [];
    _gaq.push(['_setAccount', 'UA-33818546-1']);
    _gaq.push(['_trackPageview']);
  
    (function() {
      var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
      ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
      var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
    })();
  </script>
</head>
<body>
  <a href="https://github.com/tduchateau/DataTables-Taglib">
    <img style="position: fixed; top: 0px; right: 0; border: 0; z-index: 1500;"
          src="https://s3.amazonaws.com/github/ribbons/forkme_right_red_aa0000.png"
          alt="Fork me on GitHub"></a>
  
  <div class="navbar navbar-fixed-top">
    <div class="navbar-inner">
      <div class="container">
        <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </a>
        <a class="brand" href="#" style="color: white;">
          DataTables Taglib 
          <span style="font-size: 9px;">v1.0.3</span>
        </a>
        <div class="nav-collapse">
          <ul class="nav">
            <li <% if @document.currentPage == 'index': %>class="active"<% end %>><a href="/DataTables-taglib/index.html">Overview</a></li>
            <li <% if @document.currentPage == 'features': %>class="active"<% end %>><a href="/DataTables-taglib/features.html">Features</a></li>
            <li <% if @document.currentPage == 'basic': %>class="active"<% end %>><a href="/DataTables-taglib/basic.html">Basic example</a></li>
            <li <% if @document.currentPage == 'extras': %>class="active"<% end %>><a href="/DataTables-taglib/extras.html">Extras</a></li>
            <li <% if @document.currentPage == 'reference': %>class="active"<% end %>><a href="/DataTables-taglib/reference.html">Reference</a></li>
            <li <% if @document.currentPage == 'download': %>class="active"<% end %>><a href="/DataTables-taglib/download.html">Download</a></li>
            <li <% if @document.currentPage == 'changelog': %>class="active"<% end %>><a href="/DataTables-taglib/changelog.html">Changelog</a></li>
          </ul>
          <ul class="nav pull-right">
            <li><a href="http://blog.tduchateau.fr">Blog</a></li>
          </ul>
        </div>
      </div>
    </div>
  </div>
  
  <div class="container">
    <%- @content %>
    <hr />
    <footer>
      Powered by <a href="http://www.nodejs.org/">Node.js</a> and <a href="https://github.com/bevry/docpad">DocPad</a><br />
      Designed with the not well known but promising <a href="http://twitter.github.com/bootstrap/">Twitter's Bootstrap</a> ;-)
    </footer>
  </div>
  
  <script src="/DataTables-taglib/js/fwk/jquery-1.7.2.min.js"></script>
  <script src="/DataTables-taglib/js/fwk/bootstrap.min.js"></script>
  <% for script in @document.scripts : %>
  <script src="/DataTables-taglib<%= script %>"></script>
  <% end %>
  
</body>
</html>